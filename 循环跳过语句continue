package test;

public class GetSum {
	public static void main (String args[]){
		for( int i = 1 ; i < 10 ; i++ )
		{
			if( i % 2 == 0 )
			{
				continue;
			}
			System.out.print( i+"\t" );
		}
		System.out.println();
		for( int j = 1 ; j < 10 ; j++ )
		{
			if( j % 2 == 0 )
			{
				break;
			}
			System.out.print(j+"\t");
		}
	}
}
